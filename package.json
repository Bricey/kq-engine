        "browser-sync": "^2.26.0",
        "browser-sync-webpack-plugin": "^1.2.0",
        "script-loader": "^0.7.0",
        "strip-loader": "^0.1.2",
        "typings": "^2.1.0",
        "url-loader": "^1.0.1",
        "webpack": "^2.2.1"
